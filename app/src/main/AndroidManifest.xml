<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="localdomain.nader.covtunmockup">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:screenOrientation="portrait"
            android:name="localdomain.nader.covtunapp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:screenOrientation="portrait" android:name="localdomain.nader.covtunapp.InscriptionActivity" />
        <activity
            android:screenOrientation="portrait"
            android:name="localdomain.nader.covtunapp.OpsActivity"
            android:label="@string/title_activity_ops"
            android:theme="@style/AppTheme" />
        <activity
            android:screenOrientation="portrait"
            android:name="localdomain.nader.covtunapp.DeposerOffreActivity"
            android:label="@string/title_activity_deposer_offre"
            android:theme="@style/AppTheme" />
        <activity
            android:screenOrientation="portrait"
            android:name="localdomain.nader.covtunapp.TrouverOffreActivity"
            android:label="@string/title_activity_trouver_offre"
            android:theme="@style/AppTheme" />
        <activity
            android:screenOrientation="portrait"
            android:name="localdomain.nader.covtunapp.CovoiturageDetails"
            android:label="@string/title_activity_covoiturage_details"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>